{
  "name": "treelab-lab-test",
  "version": "6.0.0",
  "scripts": {
    "clean": "rimraf build",
    "build": "yarn build:typescript && yarn build:css",
    "build:storybook": "build-storybook -o docs -s ./.storybook/images",
    "build:typescript": "tsc --project tsconfig-build.json",
    "build:css": "copyfiles -u 1 \"./src/**/*.css\" build/dist",
    "check:typescript": "tsc --noEmit",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --quiet --fix",
    "prepublishOnly": "yarn clean && yarn build",
    "storybook": "start-storybook -p 6006 -s public --no-version-updates --no-release-notes",
    "watch:typescript": "tsc --noEmit --watch"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "classnames": "^2.2.6",
    "dom-helpers": "^5.2.0",
    "jszip": "^3.4.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "rc-util": "^5.0.0",
    "react-datepicker": "^3.1.3",
    "react-dropzone": "^10.1.7",
    "react-intl": "^5.0.0",
    "react-popper-2": "npm:react-popper@^2.2.3",
    "react-textarea-autosize": "^8.2.0",
    "react-transition-group": "^4.4.1",
    "short-uuid": "^3.1.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-essentials": "^6.0.16",
    "@storybook/addon-links": "^6.0.16",
    "@storybook/addons": "^6.0.18",
    "@storybook/node-logger": "^6.0.16",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.16",
    "@storybook/theming": "^6.0.18",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.136",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-textarea-autosize": "^4.3.5",
    "@types/react-transition-group": "^4.4.0",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "copyfiles": "^2.3.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-is": "^16.9.0",
    "react-scripts": "^4.0.0",
    "rimraf": "^3.0.2",
    "storybook-addon-intl": "^2.4.1",
    "typescript": "^4.0.2"
  }
}
